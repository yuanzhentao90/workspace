package com.nashibing.tank;

public class Main {

	public static void main(String[] args) {
		TankFrame tf = new TankFrame();
		while (true) {
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			tf.repaint();
		}
	}

}
