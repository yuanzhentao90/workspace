package com.atguigu.java8;

import org.junit.Test;

/* 
 * java8四大内置核心函数式接口
	Consumer<T>：消费型接口没有返回值
		void accept(T t);
	Supplier<T>：供给型接口
		T get();
	Function<T,R>：函数型接口
		R apply(T t)
	Predicate<T>：断言型接口
		boolean test(T t)
 */

public class TestLambda3 {

	//Consumer<T>：消费型接口没有返回值
	@Test
	public void test1() {
		
	}
	
}
